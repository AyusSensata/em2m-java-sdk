{
  "match": {
    "resource": "em2m:ident:account:*"
  },
  "do": [
    {
      "match": {
        "${organization.orgType | lowerCase}": "dealer"
      },
      "allow": {
        "role": [
          "sales",
          "admin",
          "fimanager"
        ],
        "reportType": [ "Alerts" ]
      }
    },
    {
      "match": {
        "ForAllValues:StringEqualsIgnoreCase": {
          "req:role": "${role}"
        }
      },
      "assert": {
        "allow": [
          "ident:AddRoleToAccount",
          "ident:PutAccountRoles",
          "ident:RemoveRoleFromAccount"
        ]
      }
    }
  ]
}